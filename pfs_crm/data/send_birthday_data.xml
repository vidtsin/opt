<odoo>
    <data noupdate="1">
        <record id="ir_cron_birthday_email_notification" model="ir.cron">
            <field name="name">Birthday Email</field>
            <field name="user_id" ref="base.user_root" />
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False" />
            <field name="model" eval="'crm.lead'" />
            <field name="function" eval="'send_birthday_schedular'" />
            <field name="priority">1001</field>
        </record>
    </data>
</odoo>