<?xml version="1.0" encoding="utf-8"?>
<odoo>
       
    
    <template id="show_property_menu" customize_show="True" inherit_id="website.layout" name="Show Property Menu">
        <xpath expr="//ul[@id='top_menu']/li" position="before">
            <li>
                 <a t-attf-href="/properties">
                    <span>Property</span>
                </a>
            </li>
            
               
        </xpath>
    </template>
    
    
    
    
    <template id="property" name="Property" >
        <t t-call="website.layout">
            <t t-set="additional_title" t-value="property.name" />
            <div itemscope="itemscope" itemtype="http://schema.org/Property" id="wrap" class="js_sale">
                <section>

                    <div class="row">
                        <div class="col-sm-7">
                            <span itemprop="image" t-field="property.image" t-options="{'widget': 'image', 'class': 'product_detail_img', 'alt-field': 'name', 'zoom': 'image'}"/>
                            <center>
                                <a itemprop="name"  t-field="property.name" />
                            </center>
                            <span t-field="property.start_date"></span>
                            <span t-field="property.end_date"></span>
                            
                        </div>
                        <div class="col-sm-4">
                            <center>
                                <a itemprop="name"  t-field="property.name" />
                            </center>
                        </div>
                        
                    </div>
                </section>
                
            </div>
        </t>
    </template>
    
    
    <template id="property_item" name="Property item">
        <form>
            <input type="hidden" name="csrf_token" t-att-value="request.csrf_token()" />
            <div itemscope="itemscope" itemtype="http://schema.org/Property">
                <div class="oe_product_image">
                    <a itemprop="url" t-att-href="keep('/property/%s' % slug(property), page=(pager['page']['num'] if pager['page']['num']&gt;1 else None))">
                        <span itemprop="image" t-field="property.image" t-options="{'widget': 'image', 'resize': None if product_image_big else '300x300', 'zoom': 'image'}" t-att-alt="property.name" />
                    </a>
                </div>
                
                <section>
                    <h5>
                        <strong>
                            <a itemprop="name" t-att-href="keep('/property/%s' % slug(property), page=(pager['page']['num'] if pager['page']['num']&gt;1 else None))" t-field="property.name" />
                        </strong>
                    </h5>
                </section>
            </div>
        </form>
    </template>
    
 
    
