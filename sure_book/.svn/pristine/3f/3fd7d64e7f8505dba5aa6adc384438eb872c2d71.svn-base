            function checkin(id){
                
                if(id){
                    $.ajax({
                    type: 'POST',
                    url: '/dashboard_check_in',
                    timeout: 1000,
                    data : {'id': id}
                        })
                    .done(function(data){
                    if(data == 'True'){
                        window.location.reload();
                    }else {
                        alert('Error!!!');
                    }
                    })
<!--                    .fail(function(){
                        alert('Process Error!!!');
                    });-->
                }
            }
            
            
            function checkout(id){
                
                if(id){

                   $.ajax({
