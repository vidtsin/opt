            </field>
        </record>

        <act_window name="Update Room Variant"
            res_model="update.room"
            src_model="product.product"
            view_mode="form"
            target="new"
            key2="client_action_multi"
            id="action_create_room_wizard_view"/>
            
    </data>
</odoo>