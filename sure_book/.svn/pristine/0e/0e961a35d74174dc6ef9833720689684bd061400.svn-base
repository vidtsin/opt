<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        
        <record id="view_update_room" model="ir.ui.view">
            <field name="name">Update Room</field>
            <field name="model">update.room</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form>
                    <footer>
                       <button name="update_room" string="Update Room Variant" type="object" class="btn-primary"/>
