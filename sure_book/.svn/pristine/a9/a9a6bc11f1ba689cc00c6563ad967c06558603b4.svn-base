<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
         <record id="view_myallocator_channel_tree" model="ir.ui.view">
        <field name="name">myallocator.channel.tree</field>
        <field name="model">myallocator.channel</field>
        <field name="arch" type="xml">
            <tree string="Channel List">
                <field name="name" />
                <field name="channel_id" />
                <field name="currency" />
                <field name="status" />
            </tree>
        </field>
        </record>
        <record id="view_myallocator_channel_form" model="ir.ui.view">
            <field name="name">myallocator.channel.form</field>
            <field name="model">myallocator.channel</field>
            <field name="arch" type="xml">
                <form string="Channel List">
                    <sheet>
                            <group>
                                <group>
                                    <field name="channel_id"/>
                                    <field name="name"/>
                                    <field name="currency"/>
                                    <field name="status"/>
                                    <field name="allocator_config_id"/>
                                </group>
                            <group>
                                <field name="is_live"/>
                                <field name="is_beta"/>
                                <field name="require_activate"/>
                                <field name="is_calendar"/>
                                <field name="link"/>
                                <field name="is_login"/>
                            </group>
                        </group>
                        <notebook>
                            <page string="Property">
                                <field name="my_property_ids" widget="one2many_list">
                                    <tree string="Property" editable="bottom">
                                       <field name="property_id" readonly="1"/>
                                       <field name="name"/>
                                       <field name="breakfast"/>
                                       <field name="email"/>
                                    </tree>
                                </field>
                            </page>

                        </notebook>
                    
                    </sheet>
                </form>
            </field>
        </record>
        <record id="action_myallocator_channel" model="ir.actions.act_window">
            <field name="name">Channel List</field>
            <field name="res_model">myallocator.channel</field>
            <field name="type">ir.actions.act_window</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field name="view_id" ref="view_myallocator_channel_tree"/>
        </record>


        
        <menuitem action="action_myallocator_channel" id="menu_action_myallocator_channel" parent="main_menu_myallocator_property" name="Channel List" sequence="3"/>
</data>
    </odoo>