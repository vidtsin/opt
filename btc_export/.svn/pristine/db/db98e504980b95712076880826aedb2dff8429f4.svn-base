<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record model="ir.ui.view" id="view_wizard_btc_ir_attach_import">
            <field name="name">wizard.btc.ir.attach.form</field>
            <field name="model">btc.ir.attach</field>
            <field name="arch" type="xml">
                <form string="Import Ir Attachment">
                    <group>
                        <field name="import_data"/>
                    </group>

                    <footer>
                        <button string="Import" type="object" name="import_ir_attchment"/>
                    </footer>
                </form>
            </field>
        </record>


        <act_window id="action_wizard_btc_ir_attach"
                    name="Import Ir Attachment"
                    res_model="btc.ir.attach"
                    src_model="ir.attachment"
                    view_mode="form"
                    view_type="form"
                    multi="True"
                    target="new"
                    key2="client_action_multi"/>

    </data>
</odoo>

