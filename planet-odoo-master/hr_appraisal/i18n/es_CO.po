#. module: hr_appraisal
#: model:mail.template,body_html:hr_appraisal.send_appraisal_template
msgid ""
"\n"
"                \n"
"                <div style=\"font-family: 'Lucida Grande', Ubuntu, Arial, "
"Verdana, sans-serif; font-size: 12px; color: rgb(34, 34, 34); \">\n"
"                    <p>Dear ${ctx.get('employee').name},</p>\n"
"                    % if ctx.get('employee').id == object.employee_id.id:\n"
"                        <p>Please fill out the following survey related to "
"your appraisal.</p>\n"
"                    % else:\n"
"                        <p>Please fill out the following survey related to "
"${object.employee_id.name}'s appraisal.</p>\n"
"                    % endif\n"
"                    <p><a href=\"__URL__\">Click here to access the survey.</"
"a></p>\n"
"                    <p>Post your response for the appraisal till : ${object."
"date_close}</p>\n"
"                    <p>Thank you for your participation.</p>\n"
"                </div>\n"
"                \n"
"            "
msgstr ""
"\n"
"                \n"
"                <div style=\"font-family: 'Lucida Grande', Ubuntu, Arial, "
"Verdana, sans-serif; font-size: 12px; color: rgb(34, 34, 34); \">\n"
"                    <p>Estimdo ${ctx.get('employee').name},</p>\n"
"                    % if ctx.get('employee').id == object.employee_id.id:\n"
"                        <p>Por favor complete la siguiente encuesta respecto "
"a su evaluación.</p>\n"
"                    % else:\n"
"                        <p>Por favor complete la siguiente encuesta respecto "
"${object.employee_id.name}'s a su evaluación.</p>\n"
"                    % endif\n"
"                    <p><a href=\"__URL__\">Presione aquí para acceder a la "
"encuesta.</a></p>\n"
"                    <p>Publique una respuesta para la valoración hasta : "
"${object.date_close}</p>\n"
"                    <p>Gracias por su participación.</p>\n"
"                </div>\n"
"                "

