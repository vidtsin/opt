<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!--<data noupdate="1">-->
    <data noupdate="1">


        <!-- Sequences for Inspection Request -->
        <record id="seq_resource_req_form" model="ir.sequence">
            <field name="name">resource request</field>
            <field name="code">resource.request</field>
            <field name="prefix">REQNO-</field>
            <field name="padding">3</field>
            <!--<field name="request_id" eval="False"/>-->
        </record>

         <record id="seq_request_data_form" model="ir.sequence">
            <field name="name">hr resource</field>
            <field name="code">hr.resource</field>
            <field name="prefix">RESNO-</field>
            <field name="padding">3</field>
            <!--<field name="request_id" eval="False"/>-->
        </record>

         <record id="seq_category_data_form" model="ir.sequence">
            <field name="name">resource category</field>
            <field name="code">resource.category</field>
            <field name="prefix">CATEGNO-</field>
            <field name="padding">3</field>
            <!--<field name="request_id" eval="False"/>-->
        </record>



    </data>
</odoo>